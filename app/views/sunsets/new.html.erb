<h1>Add a sunset</h1>
<%= form_tag "/sunsets" do %>
  <p>
    <%= label_tag :title %><br>
    <%= text_field_tag :title %>
  </p>
  <p>
    <%= label_tag :city %><br>
    <%= text_field_tag :city %>
  </p>
  <p>
    <%= label_tag :url %><br>
    <%= text_field_tag :url %>
  </p>
  <p>
    <%= submit_tag "Save" %>
  </p>
<% end %>
